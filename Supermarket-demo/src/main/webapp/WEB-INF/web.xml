<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ListCustomerServlet</servlet-name>
  	<display-name>ListCustomerServlet</display-name>
  	<description></description>
  	<servlet-class>com.mycomapny.view.ListCustomerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddCustomerServlet</servlet-name>
  	<display-name>AddCustomerServlet</display-name>
  	<description></description>
  	<servlet-class>com.mycompany.controller.AddCustomerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ErrorServlet</servlet-name>
  	<display-name>ErrorServlet</display-name>
  	<description></description>
  	<servlet-class>com.mycomapny.view.ErrorServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SuccessServlet</servlet-name>
  	<display-name>SuccessServlet</display-name>
  	<description></description>
  	<servlet-class>com.mycomapny.view.SuccessServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddLeagueViewServlet</servlet-name>
  	<display-name>AddLeagueViewServlet</display-name>
  	<description></description>
  	<servlet-class>com.mycomapny.view.AddCustomerServlet</servlet-class>
  	<init-param>
		<param-name>customer-list</param-name>
		<param-value>Unknown, Corporate, Regular </param-value>
		</init-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ListCustomerServlet</servlet-name>
  	<url-pattern>/list_leagues.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddCustomerServlet</servlet-name>
  	<url-pattern>/add_customer.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ErrorServlet</servlet-name>
  	<url-pattern>/error.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SuccessServlet</servlet-name>
  	<url-pattern>/success.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddLeagueViewServlet</servlet-name>
  	<url-pattern>/add_customer.view</url-pattern>
  </servlet-mapping>
</web-app>
